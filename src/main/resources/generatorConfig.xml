<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<classPathEntry location="d://mysql-connector-java-5.1.6-bin.jar" />
	<context id="context" targetRuntime="MyBatis3">
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://192.168.1.10:3306/test" userId=""
			password="" />
		<javaModelGenerator targetPackage="com.supergenius.sns.entity.lucene"
			targetProject=".\srcexam" />
		<sqlMapGenerator targetPackage="com.supergenius.sns.sqlmap"
			targetProject=".\srcexam" />
		<javaClientGenerator targetPackage="com.supergenius.sns.mapper"
			targetProject=".\srcexam" type="MIXEDMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		
		<table schema="supergenius" tableName="luc_lucenekey"></table>

	</context>
</generatorConfiguration>
